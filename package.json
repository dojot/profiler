{
  "name": "dojot-profiler",
  "version": "0.1.0",
  "description": "Performance tool for dojot platform",
  "repository": {
    "type": "git",
    "url": "https://github.com/andersonluisribeiro/dojot-profiler"
  },
  "author": "Dojot",
  "license": "MIT",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-sass && npm run build-ts && npm run tslint && npm run copy-static-assets",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "build-sass": "node-sass src/public/css/main.scss dist/public/css/main.css",
    "watch-sass": "node-sass -w src/public/css/main.scss dist/public/css/main.css",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "dependencies": {
    "@types/pg": "^7.4.13",
    "@types/sequelize": "^4.27.36",
    "argparse": "^1.0.10",
    "async": "^2.6.1",
    "bcrypt-nodejs": "^0.0.3",
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "chart.js": "^2.7.3",
    "chokidar": "^2.1.1",
    "compression": "^1.7.3",
    "connect-mongo": "^2.0.3",
    "consolidate": "^0.15.1",
    "dotenv": "^4.0.0",
    "ejs": "^2.6.1",
    "errorhandler": "^1.5.0",
    "express": "^4.16.4",
    "express-fileupload": "^1.1.1-alpha.2",
    "express-flash": "0.0.2",
    "express-session": "^1.15.6",
    "express-validator": "^4.3.0",
    "io": "^1.0.0",
    "lodash": "^4.17.5",
    "mongoose": "^4.13.18",
    "mustache": "^3.0.1",
    "node-watch": "^0.6.0",
    "nodemailer": "^4.7.0",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-local": "^1.0.0",
    "pg": "^7.8.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "sequelize": "^4.42.0",
    "socket.io-client": "^2.1.1",
    "winston": "^2.4.4"
  },
  "devDependencies": {
    "@types/argparse": "^1.0.35",
    "@types/async": "^2.4.0",
    "@types/bcrypt-nodejs": "^0.0.30",
    "@types/bluebird": "^3.5.25",
    "@types/body-parser": "^1.17.0",
    "@types/chart.js": "^2.7.44",
    "@types/chokidar": "^1.7.5",
    "@types/compression": "^0.0.35",
    "@types/connect-mongo": "^0.0.35",
    "@types/dotenv": "^4.0.3",
    "@types/ejs": "^2.6.1",
    "@types/errorhandler": "^0.0.32",
    "@types/express": "^4.16.1",
    "@types/express-fileupload": "^0.4.0",
    "@types/express-session": "^1.15.11",
    "@types/jest": "^22.2.3",
    "@types/jquery": "^3.3.29",
    "@types/lodash": "^4.14.120",
    "@types/lusca": "^1.5.0",
    "@types/mongodb": "^3.1.19",
    "@types/mongoose": "^4.7.43",
    "@types/morgan": "^1.7.35",
    "@types/node": "^9.6.42",
    "@types/nodemailer": "^4.6.5",
    "@types/passport": "^0.4.7",
    "@types/passport-facebook": "^2.1.8",
    "@types/passport-local": "^1.0.33",
    "@types/request": "^2.48.1",
    "@types/request-promise": "^4.1.42",
    "@types/shelljs": "^0.7.9",
    "@types/socket.io": "^2.1.2",
    "@types/socket.io-client": "^1.4.32",
    "@types/supertest": "^2.0.7",
